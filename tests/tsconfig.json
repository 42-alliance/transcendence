{
	"compilerOptions": {
	  "target": "ES2022",  // Update from ES2024 to ES2022 (though both should work)
	  "module": "NodeNext", // Change from ESNext to NodeNext
	  "moduleResolution": "NodeNext",
	  "outDir": "./.objs",
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "strict": true,
	  "skipLibCheck": true,
	  "typeRoots": [
		"./node_modules/@types",
		"./services/@types"
	  ],
	  "resolveJsonModule": true,
	  "sourceMap": true,
	  "allowImportingTsExtensions": true,
	  "noEmit": true,
	},
	"include": [
	  "services/**/*"
	],
	"exclude": [
	  "node_modules",
	  ".objs"
	]
  }
  